<h1>Users Index</h1>

<table>
    <thead>
        <tr>Username</tr>
        <tr>Email</tr>
    </thead>

    <tbody>
        <% @users.each do |user| %>
            <tr>
                <td><%= user.username %>
                <td><%= user.email %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<%= link_to "Sign up", signup_path%>

